Set-Location HKLM:\System\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols
New-Item -Name 'SSL 3.0'
New-Item -Name 'TLS 1.0'
New-Item -Name 'TLS 1.1'
New-Item -Name 'TLS 1.2'
New-Item -Path '.\SSL 3.0' -Name 'Server'
New-Item -Path '.\TLS 1.0' -Name 'Server'
New-Item -Path '.\TLS 1.1' -Name 'Server'
New-Item -Path '.\TLS 1.2' -Name 'Server'
New-ItemProperty -Path '.\SSL 3.0\Server' -Name 'Enabled' -PropertyType 'DWord' -Value '0'
New-ItemProperty -Path '.\TLS 1.0\Server' -Name 'Enabled' -PropertyType 'DWord' -Value '1'
New-ItemProperty -Path '.\TLS 1.1\Server' -Name 'Enabled' -PropertyType 'DWord' -Value '1'
New-ItemProperty -Path '.\TLS 1.2\Server' -Name 'Enabled' -PropertyType 'DWord' -Value '1'
New-ItemProperty -Path '.\SSL 3.0\Server' -Name 'DisabledByDefault' -PropertyType 'DWord' -Value '1'
New-ItemProperty -Path '.\TLS 1.0\Server' -Name 'DisabledByDefault' -PropertyType 'DWord' -Value '0'
New-ItemProperty -Path '.\TLS 1.1\Server' -Name 'DisabledByDefault' -PropertyType 'DWord' -Value '0'
New-ItemProperty -Path '.\TLS 1.2\Server' -Name 'DisabledByDefault' -PropertyType 'DWord' -Value '0'
